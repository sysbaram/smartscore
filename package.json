{
  "name": "smartscore",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://sysbaram.github.io/smartscore/",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
